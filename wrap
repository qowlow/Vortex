    farmCoinsSection.Visible = false
    farmGemsSection.Visible = false
    teleportSection.Visible = false
    autoBlowBubbleSection.Visible = false
    autoUsePointsSection.Visible = false
end)
-- Auto use points button functionality
autoUsePointsButton.MouseButton1Click:Connect(function()
    autoUsePointsSection.Visible = true
    farmCoinsSection.Visible = false
    farmGemsSection.Visible = false
    teleportSection.Visible = false
    autoBlowBubbleSection.Visible = false
    autoSellBubbleSection.Visible = false
end)
