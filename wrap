-- Toggle button functionality
toggleButton.MouseButton1Click:Connect(function()
    if guiWindow.Visible then
        guiWindow.Visible = false
    else
        guiWindow.Visible = true
    end
end)
-- Teleport button functionality
teleportButton.MouseButton1Click:Connect(function()
    teleportSection.Visible = true
    farmCoinsSection.Visible = false
    farmGemsSection.Visible = false
    autoBlowBubbleSection.Visible = false
    autoSellBubbleSection.Visible = false
    autoUsePointsSection.Visible = false
end)
-- Island button functionality
island1Button.MouseButton1Click:Connect(function()
    -- Add teleportation code here
    print("Teleporting to Island 1...")
end)
island2Button.MouseButton1Click:Connect(function()
    -- Add teleportation code here
    print("Teleporting to Island 2...")
end)
island3Button.MouseButton1Click:Connect(function()
    -- Add teleportation code here
    print("Teleporting to Island 3...")
end)
-- Farm coins button functionality
farmCoinsButton.MouseButton1Click:Connect(function()
    farmCoinsSection.Visible = true
    teleportSection.Visible = false
    farmGemsSection.Visible = false
    autoBlowBubbleSection.Visible = false
    autoSellBubbleSection.Visible = false
    autoUsePointsSection.Visible = false
end)
-- Farm gems button functionality
farmGemsButton.MouseButton1Click:Connect(function()
    farmGemsSection.Visible = true
    farmCoinsSection.Visible = false
    teleportSection.Visible = false
    autoBlowBubbleSection.Visible = false
    autoSellBubbleSection.Visible = false
    autoUsePointsSection.Visible = false
end)
-- Auto blow bubble button functionality
autoBlowBubbleButton.MouseButton1Click:Connect(function()
    autoBlowBubbleSection.Visible = true
    farmCoinsSection.Visible = false
    farmGemsSection.Visible = false
    teleportSection.Visible = false
    autoSellBubbleSection.Visible = false
    autoUsePointsSection.Visible = false
end)
-- Auto sell bubble button functionality
autoSellBubbleButton.MouseButton1Click:Connect(function()
