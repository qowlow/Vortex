local eggHatcherButton = Instance.new("TextButton")
eggHatcherButton.Name = "Egg Hatcher Button"
eggHatcherButton.Text = "Egg Hatcher"
eggHatcherButton.Size = UDim2.new(0, 150, 0, 25)
eggHatcherButton.BackgroundTransparency = 0.5
eggHatcherButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
eggHatcherButton.Parent = buttonFolder
local autoCoinGemButton = Instance.new("TextButton")
autoCoinGemButton.Name = "Auto Coin/Gem Button"
autoCoinGemButton.Text = "Auto Coin/Gem"
autoCoinGemButton.Size = UDim2.new(0, 150, 0, 25)
autoCoinGemButton.BackgroundTransparency = 0.5
autoCoinGemButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
autoCoinGemButton.Parent = buttonFolder
-- Create key system
local key = "VORTEXKEY123"
local function checkKey()
    local userInput = game:GetService("UserInputService"):WaitForUserInputEnum()
    if userInput == key then
        print("Key accepted")
    else
        print("Invalid key")
    end
end
-- Auto Collect Chests
autoCollectChestsButton.MouseButton1Click:Connect(function()
    checkKey()
    if key == "VORTEXKEY123" then
        -- Code to auto collect chests
        print("Auto collecting chests...")
    end
end)
-- Auto Golden Egg
autoGoldenEggButton.MouseButton1Click:Connect(function()
    checkKey()
    if key == "VORTEXKEY123" then
        -- Code to auto collect golden eggs
        print("Auto collecting golden eggs...")
    end
end)
-- Unlock All Islands
unlockAllIslandsButton.MouseButton1Click:Connect(function()
    checkKey()
    if key == "VORTEXKEY123" then
        -- Code to unlock all islands
        print("Unlocking all islands...")
    end
end)
-- Auto Buy Alien Shop
autoBuyAlienShopButton.MouseButton1Click:Connect(function()
    checkKey()
    if key == "VORTEXKEY123" then
        -- Code to auto buy alien shop
        print("Auto buying alien shop...")
    end
end)
-- Egg Hatcher
eggHatcherButton.MouseButton1Click:Connect(function()
    checkKey()
    if key == "VORTEXKEY123" then
        -- Code to hatch eggs
        print("Hatching eggs...")
    end
end)
-- Auto Coin/Gem
autoCoinGemButton.MouseButton1Click:Connect(function()
    checkKey()
    if key == "VORTEXKEY123" then
        -- Code to auto collect coin/gem
        print("Auto collecting coin/gem...")
    end
