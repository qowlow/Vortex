-- Create the GUI
local gui = Instance.new("ScreenGui")
gui.Name = "VortexGUI"
gui.Parent = game.CoreGui
-- Add GUI elements
local topBar = Instance.new("Frame")
topBar.Name = "TopBar"
topBar.Size = UDim2.new(0, 600, 0, 30)
topBar.BackgroundTransparency = 0.5
topBar.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
topBar.Parent = gui
local autoBlowBubbleButton = Instance.new("TextButton")
autoBlowBubbleButton.Text = "Auto Blow Bubble"
autoBlowBubbleButton.TextSize = 14
autoBlowBubbleButton.Font = Enum.Font.SourceSansSemibold
autoBlowBubbleButton.TextColor3 = Color3.new(1, 1, 1) -- white
autoBlowBubbleButton.BackgroundTransparency = 0.5
autoBlowBubbleButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
autoBlowBubbleButton.Position = UDim2.new(0, 10, 0, 40)
autoBlowBubbleButton.Parent = topBar
local autoSellBubbleButton = Instance.new("TextButton")
autoSellBubbleButton.Text = "Auto Sell Bubble"
autoSellBubbleButton.TextSize = 14
autoSellBubbleButton.Font = Enum.Font.SourceSansSemibold
autoSellBubbleButton.TextColor3 = Color3.new(1, 1, 1) -- white
autoSellBubbleButton.BackgroundTransparency = 0.5
autoSellBubbleButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
autoSellBubbleButton.Position = UDim2.new(0, 150, 0, 40)
autoSellBubbleButton.Parent = topBar
local autoFarmCoinsButton = Instance.new("TextButton")
autoFarmCoinsButton.Text = "Auto Farm Coins"
autoFarmCoinsButton.TextSize = 14
autoFarmCoinsButton.Font = Enum.Font.SourceSansSemibold
autoFarmCoinsButton.TextColor3 = Color3.new(1, 1, 1) -- white
autoFarmCoinsButton.BackgroundTransparency = 0.5
autoFarmCoinsButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
autoFarmCoinsButton.Position = UDim2.new(0, 10, 0, 80)
autoFarmCoinsButton.Parent = topBar
local autoFarmGemsButton = Instance.new("TextButton")
autoFarmGemsButton.Text = "Auto Farm Gems"
autoFarmGemsButton.TextSize = 14
autoFarmGemsButton.Font = Enum.Font.SourceSansSemibold
autoFarmGemsButton.TextColor3 = Color3.new(1, 1, 1) -- white
autoFarmGemsButton.BackgroundTransparency = 0.5
autoFarmGemsButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
autoFarmGemsButton.Position = UDim2.new(0, 150, 0, 80)
autoFarmGemsButton.Parent = topBar
local autoUsePointsButton = Instance.new("TextButton")
autoUsePointsButton.Text = "Auto Use Points"
autoUsePointsButton.TextSize = 14
autoUsePointsButton.Font = Enum.Font.SourceSansSemibold
autoUsePointsButton.TextColor3 = Color3.new(1, 1, 1) -- white
autoUsePointsButton.BackgroundTransparency = 0.5
autoUsePointsButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
autoUsePointsButton.Position = UDim2.new(0, 10, 0, 120)
autoUsePointsButton.Parent = topBar
-- Create functions
local function autoBlowBubble()
    while true do
        -- code to blow bubble goes here
        print("Blowing bubble...")
        wait(1) -- wait 1 second before blowing the next bubble
    end
end
local function autoSellBubble()
    while true do
        -- code to sell bubble goes here
        print("Selling bubble...")
        wait(1) -- wait 1 second before selling the next bubble
    end
end
local function autoFarmCoins()
    while true do
        -- code to farm coins goes here
        print("Farming coins...")
        wait(1) -- wait 1 second before farming the next coin
    end
end
local function autoFarmGems()
    while true do
        -- code to farm gems goes here
        print("Farming gems...")
        wait(1) -- wait 1 second before farming the next gem
    end
end
local function autoUsePoints()
    while true do
        -- code to use points goes here
        print("Using points...")
        wait(1) -- wait 1 second before using the next point
    end
end
-- Attach functions to GUI elements
autoBlowBubbleButton.MouseButton1Click:Connect(function()
    local thread = coroutine.create(autoBlowBubble)
    coroutine.resume(thread)
end)
autoSellBubbleButton.MouseButton1Click:Connect(function()
    local thread = coroutine.create(autoSellBubble)
    coroutine.resume(thread)
end)
autoFarmCoinsButton.MouseButton1Click:Connect(function()
    local thread = coroutine.create(autoFarmCoins)
    coroutine.resume(thread)
end)
autoFarmGemsButton.MouseButton1Click:Connect(function()
    local thread = coroutine.create(autoFarmGems)
    coroutine.resume(thread)
end)
autoUsePointsButton.MouseButton1Click:Connect(function()
    local thread = coroutine.create(autoUsePoints)
    coroutine.resume(thread)
end)
