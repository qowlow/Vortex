-- Vortex Script
local Vortex = {}
Vortex.Version = "1.0"
Vortex.Author = "Vortex Team"
-- Load libraries
local http = game:GetService("HttpService")
local gui = game:GetService("GuiService")
local players = game:GetService("Players")
-- Create GUI
local player = players.LocalPlayer
local guiFolder = Instance.new("Folder")
guiFolder.Name = "Vortex GUI"
guiFolder.Parent = player.PlayerGui
local mainGui = Instance.new("ScreenGui")
mainGui.Name = "Main GUI"
mainGui.Parent = guiFolder
local background = Instance.new("Frame")
background.Name = "Background"
background.Size = UDim2.new(0, 300, 0, 200)
background.BackgroundTransparency = 0.5
background.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
background.Parent = mainGui
local title = Instance.new("TextLabel")
title.Name = "Title"
title.Text = "Vortex Script"
title.TextSize = 24
title.Font = Enum.Font.SourceSansSemibold
title.BackgroundTransparency = 1
title.TextColor3 = Color3.new(1, 1, 1)
title.Parent = background
-- Create buttons
local buttonFolder = Instance.new("Folder")
buttonFolder.Name = "Buttons"
buttonFolder.Parent = background
local autoCollectChestsButton = Instance.new("TextButton")
autoCollectChestsButton.Name = "Auto Collect Chests Button"
autoCollectChestsButton.Text = "Auto Collect Chests"
autoCollectChestsButton.Size = UDim2.new(0, 150, 0, 25)
autoCollectChestsButton.BackgroundTransparency = 0.5
autoCollectChestsButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
autoCollectChestsButton.Parent = buttonFolder
local autoGoldenEggButton = Instance.new("TextButton")
autoGoldenEggButton.Name = "Auto Golden Egg Button"
autoGoldenEggButton.Text = "Auto Golden Egg"
autoGoldenEggButton.Size = UDim2.new(0, 150, 0, 25)
autoGoldenEggButton.BackgroundTransparency = 0.5
autoGoldenEggButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
autoGoldenEggButton.Parent = buttonFolder
local unlockAllIslandsButton = Instance.new("TextButton")
unlockAllIslandsButton.Name = "Unlock All Islands Button"
unlockAllIslandsButton.Text = "Unlock All Islands"
unlockAllIslandsButton.Size = UDim2.new(0, 150, 0, 25)
unlockAllIslandsButton.BackgroundTransparency = 0.5
unlockAllIslandsButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
unlockAllIslandsButton.Parent = buttonFolder
local autoBuyAlienShopButton = Instance.new("TextButton")
autoBuyAlienShopButton.Name = "Auto Buy Alien Shop Button"
autoBuyAlienShopButton.Text = "Auto Buy Alien Shop"
autoBuyAlienShopButton.Size = UDim2.new(0, 150, 0, 25)
autoBuyAlienShopButton.BackgroundTransparency = 0.5
autoBuyAlienShopButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
